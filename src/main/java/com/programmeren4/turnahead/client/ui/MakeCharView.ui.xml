<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel styleName= "{style.important}" ui:field="charPanel">
		    <g:TextBox text="typ naam:" styleName= "{style.important}" ui:field="naam"></g:TextBox>
		    <g:TextBox text="typ geslacht(M/V):" styleName= "{style.important}" ui:field="geslacht"></g:TextBox>
		</g:VerticalPanel>
		<g:Button text="OK" styleName="{style.important}" ui:field="OKbutton" />
		<g:Button text="cancel" styleName="{style.important}" ui:field="cancelButton" />
	</g:HTMLPanel>
</ui:UiBinder> 